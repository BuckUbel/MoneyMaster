{
  "name": "moneymaster",
  "version": "1.0.0",
  "description": "Express, Webpack, React, Typescript(Client)",
  "main": "index.js",
  "scripts": {
    "buildDev:client": "webpack --mode development --config webpack.dev.config.js",
    "buildProd:client": "webpack --mode production --config webpack.prod.config.js",
    "building:client": " webpack --mode development --watch --config webpack.dev.config.js ",
    "server": "nodemon -L ./private/index.js",
    "preBuild:server": "tslint -c tslint.server.json -p tsconfig.server.json --fix",
    "build:server": "tsc -p tsconfig.server.json -w",
    "start:server": "nodemon %NODE_DEBUG_OPTION% -L --watch ./private --delay 2.5 ./private/index.js"
  },
  "license": "ISC",
